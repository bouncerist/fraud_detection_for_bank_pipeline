{
  "name": "src_dwh",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.dbname": "bank_storage",
    "database.user": "postgres",
    "database.hostname": "bank_storage",
    "database.password": "postgres",
    "database.port": "5432",
    "database.schema": "public",
    "database.server.name": "prj",
    "topic.prefix": "bank",
    "topic.creation.default.cleanup.policy": "delete",
    "topic.creation.default.partitions": 3,
    "topic.creation.default.replication.factor": 3,
    "slot.name": "dbz_slot_all_tables",
    "plugin.name": "pgoutput",
    "publication.name": "dbz_bank_publication",
    "database.history.kafka.bootstrap.servers": "kafka-1:29092,kafka-2:29092,kafka-3:29092",
    "database.history.kafka.topic": "schema-changes.from_db",
    "snapshot.mode": "initial",
    "snapshot.select.statement.overrides":  "public.clients, public.cards, public.card_types, public.merchants, public.transaction_details, public.transactions",
    "snapshot.select.statement.overrides.public.clients": "SELECT * FROM public.\"clients\" LIMIT 1",
    "snapshot.select.statement.overrides.public.cards": "SELECT * FROM public.\"cards\" LIMIT 1",
    "snapshot.select.statement.overrides.public.card_types": "SELECT * FROM public.\"card_types\"",
    "snapshot.select.statement.overrides.public.merchants": "SELECT * FROM public.\"merchants\"",
    "snapshot.select.statement.overrides.public.transactions": "SELECT * FROM public.\"transactions\" LIMIT 1",
    "snapshot.select.statement.overrides.public.transaction_details": "SELECT * FROM public.\"transaction_details\" LIMIT 1",
    "table.include.list": "public.clients, public.cards, public.card_types, public.merchants, public.transaction_details, public.transactions",
    "transforms": "unwrap,timestampConverter",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "transforms.timestampConverter.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.timestampConverter.target.type": "string",
    "transforms.timestampConverter.field": "transaction_date",
    "transforms.timestampConverter.format": "yyyy-MM-dd'T'HH:mm:ss.SSS",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "http://schema-registry-1:8081,http://schema-registry-2:8081,http://schema-registry-3:8081",
    "key.converter": "io.confluent.connect.avro.AvroConverter",
    "key.converter.schema.registry.url": "http://schema-registry-1:8081,http://schema-registry-2:8081,http://schema-registry-3:8081",
    "time.precision.mode": "connect"
  }
}
